{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "rootDir": "extension",
    "outDir": "./built",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "strict": true,
    "allowJs": false,
    "sourceMap": false,
    "typeRoots": [ "node_modules/@types" ]
  }
}